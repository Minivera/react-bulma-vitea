{
  "name": "react-vitae-bulma-theme",
  "version": "1.0.6",
  "description": "The Bulma Theme for react-vitae, build resumes using the bloomer framework and react-vitae.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minivera/react-vitae.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsdx build",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "prepublishOnly": "npm run build",
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "clean": "git clean -fdX ."
  },
  "keywords": [
    "react",
    "resume",
    "json-resume",
    "bulma",
    "theme"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/react-vitae-bulma-theme.umd.production.js",
  "module": "dist/react-vitae-bulma-theme.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Guillaume St-Pierre",
  "license": "MIT",
  "resolutions": {
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4"
  },
  "peerDependencies": {
    "bloomer": "^0.6.5",
    "react": ">=17",
    "react-vitae": "^1.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "@testing-library/react/dont-cleanup-after-each"
    ]
  },
  "gitHead": "484532fc4bea32256072fde2bee599ecbceae98f",
  "devDependencies": {
    "react-vitae": "^1.0.6"
  }
}
